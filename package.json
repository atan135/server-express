{
  "name": "express-mysql-project",
  "version": "1.0.0",
  "description": "Express project with log4js, auth middleware and MySQL",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "start:dev": "cross-env NODE_ENV=development node src/server.js",
    "start:prod": "cross-env NODE_ENV=production node src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:debug": "jest --listTests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "event-loop-lag": "^1.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "mysql2": "^3.14.3",
    "redis": "^4.6.13"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.js"
    ],
    "verbose": true
  }
}